{
  "expo": {
    "name": "optimal",
    "slug": "optimal",
    "version": "1.0.0",
    "icon": "./assets/optimal_icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/optimal_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#181818"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "optimal-dev",
      "requiresFullScreen": true,
      "infoPlist": {
        "UISupportedInterfaceOrientations": ["UIInterfaceOrientationPortrait"],
        "UISupportedInterfaceOrientations~ipad": ["UIDeviceOrientationPortrait"]
      }
    },

    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.robertharrington.optimal",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/optimal_icon.png",
        "backgroundColor": "#181818"
      }
    },
    "web": {
      "favicon": "./assets/optimal_icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "79d34850-acd3-4796-a1d1-589c543d576c"
      }
    }
  }
}
